{
  "version": "0.1.0",
  "dependencies": {
    "aws-sdk": "2.878.0",
    "pg": "8.5.1",
    "pg-native": "3.0.0",
    "reflect-metadata": "0.1.13",
    "source-map-support": "^0.5.19",
    "typeorm": "0.2.32",
    "typeorm-aurora-data-api-driver": "2.1.0",
    "typeorm-naming-strategies": "2.0.0"
  },
  "scripts": {
    "example": "ts-node src/example.ts",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js --config src/config.ts migration:create --name seed",
    "migration:show": "ts-node ./node_modules/typeorm/cli.js --config src/config.ts migration:show",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js --config src/config.ts migration:revert",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js --config src/config.ts migration:run"
  },
  "devDependencies": {
    "@types/pg": "7.14.11",
    "@types/node": "14.14.37",
    "dotenv": "8.2.0",
    "eslint": "^7.21.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "^4.2.3"
  }
}
